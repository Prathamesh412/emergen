<head>
        <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
</head>
<div class="container">
    <header class="jumbotron">
        <div class="container">
        <h1>This is the image page</h1>
        <p><a href="/" class="btn btn-primary btn-large">GO TO main Page</a></p>
        </div>
    </header>
<div class="row">
<% users.forEach(function(user){ %>
    <div class="col-md-4 col-sm-6">
        <div class="img-thumbnail">
            <img src="../../uploads/<%=user.image%>" class="img-responsive" alt="image" width="100%" height="auto">
                <div class="caption">
                    <h4><b>Name:-</b> <%= user.name %></h4>
                    <h4><b>Email:-</b> <%= user.email %></h4>
                    <h4><b>Phone:-</b> <%= user.phone %></h4>
                    <h4><b>Message:-</b> <%= user.message %></h4>
                    <h4><b>Date:-</b> <%= user._id.getTimestamp()%></h4>
                </div>
                <p>
                    <a href="/aryan1993/<%= user._id %>" class="btn btn-primary">More Info</a>
                </p>
        </div>
    </div>
<% }); %>
</div> 
<!-- Row for user loop ends -->
<% if (pages > 0) { %>
    <ul class="pagination text-center">
        <% if (current == 1) { %>
            <li class="disabled"><a>First</a></li>
        <% } else { %>
            <li><a href="/aryan1993/images/1">First</a></li>
        <% } %>
        <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
        <% if (i !== 1) { %>
            <li class="disabled"><a>...</a></li>
        <% } %>
        <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
            <% if (i == current) { %>
                <li class="active"><a><%= i %></a></li>
            <% } else { %>
                <li><a href="/aryan1993/images/<%= i %>"><%= i %></a></li>
            <% } %>
            <% if (i == Number(current) + 4 && i < pages) { %>
                <li class="disabled"><a>...</a></li>
            <% } %>
        <% } %>
        <% if (current == pages) { %>
            <li class="disabled"><a>Last</a></li>
        <% } else { %>
            <li><a href="/aryan1993/images/<%= pages %>">Last</a></li>
        <% } %>
    </ul>
<% } %>


</div>
<p>
        <a href="/" class="btn btn-primary">Back to home page</a>
</p>